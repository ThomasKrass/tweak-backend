  "homepage": "https://github.com/ThomasKrass/tweak#readme",
  "engines": {
    "node": "18.x"
  },
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "npm run prod",
    "dev": "npm run build && cross-env NODE_ENV=development node --experimental-specifier-resolution=node dist/index.js",
